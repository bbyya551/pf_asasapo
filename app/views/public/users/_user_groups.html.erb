<table class='table table-borderless'>
  <thead>
    <tr>
      <th></th>
      <th>コミュニティ名</th>
      <th>メンバー数</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% user.groups.each do |group| %>
      <tr>
        <td><%= image_tag group.get_image(50, 50), class:"rounded-circle"; %></td>
        <td><%= link_to group.name, group_path(group.id) %></td>
        <td><%= group.group_users.count %>人</td>
      </tr>
    <% end %>
  </tbody>
</table>