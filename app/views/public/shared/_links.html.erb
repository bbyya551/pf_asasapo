<%- if controller_name != 'sessions' %>
  <%= link_to "会員の方はこちら", new_session_path(resource_name) %><br />
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "新規会員登録はこちら", new_registration_path(resource_name), class: 'btn btn-outline-warning btn-block rounded-pill mt-3' %><br />
<% end %>

